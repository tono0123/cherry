<p id="notice"><%= notice %></p>

<h1>Listing Users</h1>

<table>
  <thead>
    <tr>
      <th>Company</th>
      <th>Sei</th>
      <th>Mei</th>
      <th>Postal code</th>
      <th>Adress</th>
      <th>Mail</th>
      <th>Tel</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.company %></td>
        <td><%= user.sei %></td>
        <td><%= user.mei %></td>
        <td><%= user.postal_code %></td>
        <td><%= user.adress %></td>
        <td><%= user.mail %></td>
        <td><%= user.tel %></td>
        <td><%= link_to 'Show', user %></td>
        <td><%= link_to 'Edit', edit_user_path(user) %></td>
        <td><%= link_to 'Destroy', user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User', new_user_path %>
